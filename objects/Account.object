    NOT(ISPICKVAL(Type__pc, &apos;GENERIC&apos;)),
    OR(NOT(ISBLANK(FirstName)), NOT(ISBLANK(First_Name_Kanji__pc))),
    OR(NOT(ISBLANK(LastName)), NOT(ISBLANK(Last_Name_Kanji__pc))),
    NOT(ISPICKVAL(Salutation, &quot;&quot;)),
    NOT(ISPICKVAL(Market__pc, &quot;&quot;)),
    OR(
        AND(NOT(ISBLANK(PersonEmail)), Opt_in_Email__pc),
        AND(NOT(ISBLANK(PersonMobilePhone)), Opt_in_SMS__pc),
        AND(NOT(ISBLANK(PersonMobilePhone)), Opt_in_Phone__pc),
        AND(
