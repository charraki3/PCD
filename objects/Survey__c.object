        <fullName>Promotor__c</fullName>
        <description>Technical field used for reporting. Formula which shows the value &quot;1&quot; when Recommendation Level is &quot;Promotor&quot;, and &quot;0&quot; if not.</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Recommendation_Level__c, &quot;Promotor&quot;),1,
0
)</formula>
        <label>Promotor</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
