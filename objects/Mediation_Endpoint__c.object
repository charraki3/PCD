<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Holds the credentials required to call mediation on an outbound flow: URL (retrieved from BizTalk interface contract) and endpoint Primary key (retrieved from PCIS dev portal under user profile)</description>
    <enableFeeds>false</enableFeeds>
    <label>Mediation Endpoint</label>
    <visibility>Public</visibility>
    <fields>
        <fullName>Url__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>Url of endpoint to be called</description>
        <externalId>false</externalId>
        <label>Url</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Primary_Key__c</fullName>
        <description>Primary key to be used under the header &quot;Ocp-Apim-Subscription-Key&quot; in the http request</description>
        <externalId>false</externalId>
        <label>Primary Key</label>
        <length>50</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
</CustomObject>